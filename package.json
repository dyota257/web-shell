{
	"scripts": {
		"start": "nodemon dist/main.js",
		"cli": "ts-node src/cli.ts",
		"test": "jest",
		"lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
		"build": "tsc -p tsconfig.json",
		"clean": "rm -rf dist build",
		"ts-node": "ts-node",
		"dev": "nodemon --config nodemon.json src/index.ts && start \"http://localhost:8000/\"",
		"dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
		"postinstall": "tsc",
		"watch-node": "nodemon dist/index.js",
		"watch-ts": "tsc -w"
	},
	"devDependencies": {
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.9.0",
		"esbuild": "^0.14.10",
		"eslint": "^8.6.0",
		"jest": "^27.4.7",
		"ts-jest": "^27.1.2",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@types/express": "^4.17.13",
		"axios": "^0.24.0",
		"body-parser": "^1.19.1",
		"ejs": "^3.1.6",
		"express": "^4.17.2",
		"tslib": "^2.3.1"
	}
}
